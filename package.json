{
  "name": "kanedama",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MansaGroup/kanedama.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MansaGroup/kanedama/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        2,
        "always",
        85
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{md,yml,html,css,scss,json}": "prettier --write"
  },
  "homepage": "https://github.com/MansaGroup/kanedama#readme",
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "eslint": "7.28.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-import-helpers": "1.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-sonarjs": "0.7.0",
    "eslint-plugin-unused-imports": "1.1.1",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1"
  }
}
